{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run theme-serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "theme-serve": "node src/themeNodeServe/serve.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.4.0",
    "element-ui": "^2.12.0",
    "import-vue": "0.0.9",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "pubsub-js": "^1.7.0",
    "qrcodejs2": "^0.0.2",
    "typescript": "^3.7.5",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass-resources-loader": "^2.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "indent": [
        2,
        4
      ],
      "strict": 2,
      "no-redeclare": 0,
      "vetur.validation.template": false,
      "vue/no-use-v-if-with-v-for": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "$ThemeBaseUrl": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
